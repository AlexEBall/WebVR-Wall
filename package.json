{
  "name": "WebVR-Wall",
  "version": "1.0.0",
  "description": "Motivational App built with React and Aframe",
  "dependencies": {
    "aframe": "^0.7.1",
    "aframe-react": "4.3.0",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "2.2.5",
    "express": "^4.16.2",
    "fetch-jsonp": "1.0.6",
    "immutable": "^3.8.1",
    "mongoose": "^5.0.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-scripts": "^1.1.0",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "sass-loader": "^6.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile:sass": "node-sass client/public/sass/main.scss client/public/css/style.css -w",
    "serve": "cd src && python -m SimpleHTTPServer 8081",
    "serve-js": "webpack-dev-server --content-base src --progress --watch --history-api-fallback --inline --hot --port 8082"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexEBall/WebVR-Wall"
  },
  "keywords": [
    "aframe",
    "react",
